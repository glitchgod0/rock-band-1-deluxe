(send_hit
   ($streak $score $crowd)
   {do
      ($player_num
         {$this player_num})
      {session
         send_to_all
         {'`'
            (z
               _
               kRemoteHit
               {',' $player_num}
               {',' $streak}
               {',' $score}
               {','
                  {frac $crowd}})}
         kNetUnreliable}})
