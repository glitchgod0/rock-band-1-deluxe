#define CROWD_AUDIO_HANDLERS
((results_duck 9)
   (results_fade_sec 10)
   (intro_start
      {if
         {$this want_intro}
         {crowd_begin play}}
      {$this play_intro})
   (intro_skip
      {if
         {$this want_intro}
         {crowd_begin play}}
      {$this play_intro})
   (want_intro
      {'||'
         {!
            {tour performer}}
         {{tour performer}
            first_song}})
   (want_outro_duck
      {'||'
         {!
            {tour performer}}
         {{tour performer}
            last_song}})
   (game_lost
      {crowd_lose play}
      {$this on_lose})
   (music_end
      {unless
         {$this done}
         {do
            ($cue win_5.cue)
            {if
               {gamemode is_band_arrangement}
               {do
                  ($num_stars
                     {{band_0 main_performer}
                        num_stars})
                  {switch
                     $num_stars
                     (5
                        {set $cue win_5.cue})
                     (4
                        {set $cue win_4.cue})
                     {set $cue win_4.cue}}}}
            {$cue play}}
         {$this on_win}})
   (coda_success
      {bre_success.cue stop}
      {bre_success.cue play})
   (coda_blown
      {bre_fail.cue play})
   (final_win
      ($type)
      {do
         ($strm
            {switch
               $type
               (all
                  "wingame_band")
               (drum
                  "wingame_drums")
               (guitar
                  "wingame_guitar")
               "wingame_vocals"})
         {$this play_stream 0 $strm}}))
