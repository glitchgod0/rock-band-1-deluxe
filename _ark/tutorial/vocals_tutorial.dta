      ($beat $beat_count))
   (tutorial_states
      (invalid_state loading_screen back_to_main_menu back_to_lesson_menu basics_intro basics_singing basics_nonpitched basics_mechanics basics_rhythm basics_starpower basics_wrapup))
   (tutorial_states_ambient
      (FALSE FALSE FALSE FALSE FALSE TRUE TRUE TRUE TRUE TRUE TRUE))
