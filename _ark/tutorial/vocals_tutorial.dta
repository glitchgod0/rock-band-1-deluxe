      (FALSE FALSE FALSE FALSE TRUE TRUE TRUE TRUE TRUE TRUE FALSE))
   (tutorial_states_can_dec
      (FALSE FALSE FALSE FALSE FALSE TRUE TRUE TRUE TRUE TRUE TRUE))
