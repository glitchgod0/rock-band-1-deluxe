{new
   CharLoadingPanel
   tour_band_hub_panel
   (file tour_band_hub.milo)
   (reset TRUE)
   LIMIT_TO_SESSION
   (enter
      {if
         [reset]
         {$this set_focus gigguide.btn}}
      {set
         [reset]
         FALSE}
      {if
         {==
            {gamecfg get leader_num}
            -1}
         {$this disable gigguide.btn}
         {$this disable bandprofile.btn}
         {$this disable bandlineup.btn}
         {$this set_focus rokkshoppe.btn}}
      {$this refresh}
      {unless
         {==
            {session mode}
            kNetLocal}
         {game
            foreach_local_player_config
            $pcfg
            {$pcfg set_ui_waiting TRUE}}}
      {practice_space_panel refresh})
   (TRANSITION_COMPLETE_MSG
      {band_ui_sink set_net_sync FALSE}
      {band_ui_sink set_block_mode kBlockNone}
      {autosave}
      {unless
         {'||'
            {==
               {ui current_screen}
               ''}
            {==
               {gamecfg get leader_num}
               -1}}
         {do
            ($p_profile
               {profile_mgr
                  get_mapped_profile_from_player
                  {gamecfg get leader_num}})
            {if
               {&&
                  $p_profile
                  {$p_profile is_first_hub_visit}}
               {ui push_screen hub_welcome_screen}}}})
   (refresh
      {if_else
         {==
            {{tour performer}
               world_fame}
            0}
         {gigguide.btn set text_token tour_gigguide_start}
         {gigguide.btn set text_token tour_gigguide_continue}}
      {bandname.lbl
         set_localized
         {{tour performer}
            band_name}}
      {do
         ($perf
            {tour performer})
         {stats.grp
            set_showing
            {!=
               {$perf world_fame}
               0}}
         {fame.lbl
            set_localized
            {fmt_score
               {$perf world_fame}}}
         {buzz.lbl
            set_localized
            {fmt_score
               {$perf fans}}}}
      {foreach_int
         $slot
         0
         {player_names.set
            size
            (objects)}
         {do
            ($name
               {player_names.set
                  get
                  (objects $slot)})
            ($part
               {player_parts.set
                  get
                  (objects $slot)})
            ($money
               {player_money.set
                  get
                  (objects $slot)})
            ($leader
               {player_leaders.set
                  get
                  (objects $slot)})
            ($no_instrument
               {player_no_instruments.set
                  get
                  (objects $slot)})
            ($player_num
               {game get_player_num_from_slot_num $slot})
            ($active
               {!= $player_num -1})
            {$name set_showing $active}
            {$money set_showing FALSE}
            {$part set_showing $active}
            {$leader
               set_showing
               {&&
                  $active
                  {==
                     $player_num
                     {game get leader_num}}}}
            {$no_instrument
               set_showing
               {! $active}}
            {if_else
               $active
               {do
                  ($char
                     {{game get_player_config $player_num}
                        get_char})
                  {$name
                     set_localized
                     {$char get_char_name}}
                  {$part
                     set_localized
                     {$char instrument_icon}}
                  {$money
                     set_localized
                     {fmt_money
                        {$char money}}}}
               {$no_instrument
                  set_localized
                  {cnv_instrumenttoicon
                     {elem
                        (COOP_SLOTS_ORDER)
                        $slot}}}}}})
   (SELECT_START_MSG
      {switch
         $component
         (gigguide.btn
            {if
               {>=
                  {game get_num_participants}
                  {gamemode get min_num_participants}}
               {if_else
                  {==
                     {session mode}
                     kNetLocal}
                  {tour_band_waiting_panel advance}
                  {ui goto_screen tour_band_waiting_screen}}})
         (rokkshoppe.btn
            {band_ui_sink set_block_mode kBlockShop}
            {shop_char_panel set char_player_num $player_num}
            {shop_panel set reset_on_enter TRUE}
            {ui goto_screen shop_screen})
         (bandprofile.btn
            {band_ui_sink set_block_mode kBlockShop}
            {ui goto_screen tour_band_profile_screen})
         (bandlineup.btn
            {band_ui_sink set_block_mode kBlockShop}
            {ui goto_screen tour_band_lineup_screen})
         (playerprofile.btn
            {band_ui_sink set_block_mode kBlockShop}
            {tour_char_profile_panel set player_num $player_num}
            {tour_char_profile_panel set profile_player_num $player_num}
            {ui goto_screen tour_band_char_profile_screen})
         (community.btn
            {if_else
               {server is_connected}
               {do
                  {band_ui_sink set_block_mode kBlockShop}
                  {leaderboards_netwatcher_panel set return_screen tour_band_hub_screen}
                  {ui goto_screen tour_hub_community_screen}}
               {ui push_screen leaderboards_not_available_screen}})}
      kDataUnhandled)
   (SELECT_MSG
      {switch
         $component
         (gigguide.btn
            {if
               {<
                  {game get_num_participants}
                  {gamemode get min_num_participants}}
               {ui push_screen tour_band_min_participants_screen}})})}
