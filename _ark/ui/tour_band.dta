{new
   DialogPanel
   tour_band_carrot_panel
   (file
      "tour_band_carrot.milo")
   LIMIT_TO_LEADER_SESSION
   (helpbar
      ('' helpbar_continue))
   (enter
      {carrot_item.lbl
         set
         text_token
         {$this
            carrot_item_token
            "manager"}}
      {carrot_desc.lbl
         set_localized
         {$this
            carrot_desc_str
            "manager"}})
   (carrot_item_token
      ($prefix)
      {do
         ($tp
            {tour performer})
         ($carrot_event
            {$tp next_item_event})
         ($item
            {$carrot_event get award_item})
         #ifndef _SHIP
         {unless
            $carrot_event
            {fail
               "no $carrot_event!"}}
         #endif
         {if_else
            {==
               kTourNew
               {$tp event_status $carrot_event}}
            {sprintf
               "%s%s_carrot_canwin"
               $prefix
               $item}
            {sprintf
               "%s%s_carrot_needmore"
               $prefix
               $item}}})
   (carrot_desc_str
      ($prefix)
      {do
         ($tp
            {tour performer})
         ($carrot_event
            {$tp next_item_event})
         ($item
            {$carrot_event get award_item})
         #ifndef _SHIP
         {unless
            $carrot_event
            {fail
               "no $carrot_event!"}}
         #endif
         {if_else
            {==
               kTourNew
               {$tp event_status $carrot_event}}
            ""
            {do
               ($world_fame
                  {$tp world_fame})
               ($required_world_fame
                  {$carrot_event get world_fame_required})
               ($town
                  {$tp find_event_town $carrot_event})
               ($town_fame
                  {$tp town_fame $town})
               ($required_town_fame
                  {$carrot_event get town_fame_required})
               ($fans
                  {$tp fans})
               ($required_fans
                  {$tp
                     buzz_to_fans
                     {$carrot_event get buzz_required}})
               {cond
                  ({> $required_town_fame $town_fame}
                     {sprintf
                        {localize
                           {sprintf
                              "%stour_band_need_townfame"
                              $prefix}}
                        {localize_token $town}})
                  ({> $required_world_fame $world_fame}
                     {localize_token
                        {sprintf
                           "%stour_band_need_worldfame"
                           $prefix}})
                  ({> $required_fans $fans}
                     {localize_token
                        {sprintf
                           "%stour_band_need_fans"
                           $prefix}})
                  (TRUE
                     {fail
                        "Could not determine carrot desc!"})}}}})
   (BUTTON_DOWN_MSG
      {if
         {'||'
            {== $button kPad_Start}
            {== $button kPad_X}}
         {synth play button_select}
         {ui pop_screen}})}
