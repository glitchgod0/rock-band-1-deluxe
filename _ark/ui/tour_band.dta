{new
   BandCreatePanel
   tour_band_create_panel
   (file tour_name_band.milo)
   (focus name.btn)
   (reset_focus TRUE)
   LIMIT_TO_LEADER_SESSION
   (enter
      {hometown.lst
         set_provider
         {tour hometown_provider}}
      {$this update_band})
   (exit
      {band_ui_sink hide_network_busy_icon})
   (update_band
      {if_else
         {==
            {{tour performer}
               band_name}
            ''}
         {name.lbl set text_token shell_enter_name}
         {name.lbl
            set_localized
            {{tour performer}
               band_name}}}
      {hometown.lst
         set_selected
         {{tour performer}
            hometown}})
   (SCROLL_MSG
      {switch
         $component
         (hometown.lst
            {{tour performer}
               set_hometown
               {hometown.lst selected_sym}})})
   (SELECT_MSG
      {switch
         $component
         (name.btn
            {platform_mgr
               show_keyboard
               $player_num
               {get_band_name_length}
               {localize create_band_vk_title}
               {localize create_band_vk_desc}
               {if_else
                  {==
                     {{tour performer}
                        band_name}
                     ''}
                  {$this random_band_name}
                  {{tour performer}
                     band_name}}
               $this})
         (hometown.btn
            {hometown.lst store}
            {$this set_focus hometown.lst})
         (hometown.lst
            {hometown.lst confirm}
            {$this set_focus hometown.btn})
         (accept.btn
            {cond
               ({==
                     {{tour performer}
                        band_name}
                     ""}
                  {ui push_screen tour_band_name_error_screen})
               ({!
                     {profile_mgr
                        is_band_name_unique
                        {gamecfg get leader_num}
                        {{tour performer}
                           band_name}}}
                  {ui push_screen tour_band_create_name_not_unique_screen})
               (TRUE
                  {{tour performer}
                     init}
                  {tour
                     give_new_band_to_profile
                     {profile_mgr
                        get_mapped_profile_from_player
                        {gamecfg get leader_num}}}
                  {if_else
                     {profile_mgr get_all_unlocked}
                     {do
                        {if
                           {{tour performer}
                              is_local}
                           {{tour performer}
                              update_members}}
                        {tour_band_hub_panel set reset TRUE}
                        {ui goto_screen tour_band_hub_screen}}
                     {do
                        {band_ui_sink show_network_busy_icon}
                        {entity_uploader
                           update_band
                           {tour performer}
                           $this}}})})
         (cancel.btn
            {ui
               goto_screen
               {gamemode get matchmaking_screen}})})
   (VIRTUAL_KEYBOARD_RESULT_MSG
      {if
         {&&
            $ok
            $this
            {ui focus_panel}
            {==
               {ui focus_panel}
               $this}}
         {{tour performer}
            set_band_name
            $text}
         {$this update_band}})
   (ROCK_CENTRAL_OP_COMPLETE_MSG
      {band_ui_sink hide_network_busy_icon}
      {switch
         $arg1
         (RB_RETCODE_BAND_NAME_PROFANE
            {ui push_screen tour_band_name_profane_screen})
         (RB_RETCODE_BAND_NAME_DUP
            {ui push_screen tour_band_name_dup_screen})
         (RB_RETCODE_BAND_NAME_IP
            {ui push_screen tour_band_name_ip_screen})
         {if
            {{tour performer}
               is_local}
            {{tour performer}
               update_members}}
         {tour_band_hub_panel set reset TRUE}
         {ui goto_screen tour_band_hub_screen}})
   (BUTTON_DOWN_MSG
      {cond
         ({band_ui_sink is_network_busy}
            TRUE)
         ({== $button kPad_Tri}
            {switch
               {$this focus_name}
               (hometown.lst
                  {hometown.lst undo $player_num}
                  {$this set_focus hometown.btn})
               kDataUnhandled})
         (TRUE kDataUnhandled)})}
