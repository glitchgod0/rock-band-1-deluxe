{new
   UIPanel
   tour_solo_event_panel
   LIMIT_TO_LEADER_SESSION
   TOUR_EVENT_PANEL_HANDLERS
   COMMON_SONG_PREVIEW
   (file tour_solo_event.milo)
   (focus event.lst)
   (enter
      {$this select_first_unplayed}
      {$this refresh}
      {$this play_preview}
      {platform_mgr disable_xmp})
   (select_first_unplayed
      {do
         ($mode
            {{tour solo_event_provider}
               get
               mode})
         {unless
            {== $mode kTourVenueDownload}
            {do
               ($event
                  {{tour performer}
                     first_unplayed_event
                     $mode})
               {if
                  $event
                  {event.lst
                     set_provider
                     {tour solo_event_provider}}
                  {event.lst
                     set_selected
                     {$event get event_name}}
                  {$this update_selection}}}}})
   (exit
      {song_preview ''}
      {session remove_sink $this})
   (refresh
      {do
         ($provider
            {tour solo_event_provider})
         {event.lst set_provider $provider}
         {if
            {tour event}
            {event.lst
               set_selected
               {{tour event}
                  get
                  event_name}
               FALSE
               {$provider refresh_first_showing}}}}
      {$this update_selection}
      {if
         {tour hint}
         {ui
            goto_screen
            {{tour hint}
               get
               screen}}})
   (update_selection
      {do
         ($provider
            {tour solo_event_provider})
         ($pos
            {event.lst selected_pos})
         ($town
            {$provider town $pos})
         ($venue
            {$provider venue $pos})
         {{tour performer}
            set_town
            $town}
         {{tour performer}
            set_venue
            $venue}
         {tour_world_bg_panel set town $town}
         {tour_world_bg_panel set venue $venue}
         {tour_world_bg_panel zoom_in}
         {unless
            {$this is_mode_selected}
            {{tour performer}
               set_event
               {event.lst selected_sym}
               {game get leader_num}}}})
   (BUTTON_DOWN_MSG
      {cond
         ({== $button kPad_X}
            {do
               ($selected
                  {event.lst selected_sym})
               {if_else
                  {&&
                     {!
                        {$this is_mode_selected}}
                     {==
                        kTourLocked
                        {{tour performer}
                           event_status}}}
                  TRUE
                  kDataUnhandled}})
         ({== $button kPad_Tri}
            {do
               ($prev_mode
                  {{tour solo_event_provider}
                     get
                     mode})
               {platform_mgr enable_xmp}
               {if_else
                  {== $prev_mode kTourVenueNormal}
                  kDataUnhandled
                  {do
                     {synth play button_back}
                     {{tour solo_event_provider}
                        set
                        mode
                        kTourVenueNormal}
                     {event.lst
                        set_provider
                        {tour solo_event_provider}}
                     {event.lst
                        set_selected
                        {if_else
                           {== $prev_mode kTourVenueDownload}
                           download
                           unlock}}
                     {$this update_selection}
                     {$this play_preview}}}})
         ({$this is_updown $button}
            {song_preview ''}
            kDataUnhandled)
         (TRUE kDataUnhandled)})
   (BUTTON_UP_MSG
      {if
         {&&
            {!
               {ui in_transition}}
            {$this is_updown $button}}
         {$this play_preview}}
      kDataUnhandled)
   (play_preview
      {if_else
         {'||'
            {$this is_mode_selected}
            {==
               kTourLocked
               {{tour performer}
                  event_status}}}
         {song_preview ''}
         {song_preview
            {{tour solo_event_provider}
               event_song
               {event.lst selected_pos}}}})
   (is_mode_selected
      {'||'
         {==
            unlock
            {event.lst selected_sym}}
         {==
            download
            {event.lst selected_sym}}})
   (SCROLL_MSG
      {if
         {!=
            {{tour solo_event_provider}
               town
               {event.lst selected_pos}}
            {{tour performer}
               town}}
         {synth play bwt_swish.cue}}
      {$this update_selection})
   (SELECT_START_MSG
      {if
         {&&
            {!=
               {event.lst selected_sym}
               unlock}
            {!=
               {event.lst selected_sym}
               download}
            {!=
               kTourLocked
               {{tour performer}
                  event_status}}}
         {$this confirm_event}
         {do
            ($pcfg
               {game
                  get_player_config
                  {game get leader_num}})
            {$pcfg
               set_track_type
               {{$pcfg get_char}
                  get_instrument}}}
         {session add_sink $this}
         {{game
               get_player_config
               {game get leader_num}}
            set_ready
            TRUE}}
      kDataUnhandled)
   (start_loading_msg
      {ui goto_screen loading_screen})
   (SELECT_MSG
      {switch
         {event.lst selected_sym}
         (unlock
            {{tour solo_event_provider}
               set
               mode
               kTourVenueUnlock}
            {$this select_first_unplayed}
            {$this refresh}
            {$this play_preview})
         (download
            {{tour solo_event_provider}
               set
               mode
               kTourVenueDownload}
            {$this refresh}
            {$this play_preview})})}
