{new
   CharLoadingPanel
   tour_solo_char_panel
   (file
      "tour_solo_char.milo")
   (focus char.lst)
   (reset_focus TRUE)
   LIMIT_TO_LEADER_SESSION
   (char_provider
      {object
         ""})
   (enter
      {band_ui_sink set_controller_level kControllerLevelSession}
      {do
         ($pcfg
            {game
               get_player_config
               {game get leader_num}})
         {$pcfg set_char 0}
         {gamertag.lbl
            set_localized
            {$pcfg get player_name}}
         {instrument.lbl
            set_localized
            {get_font_char_from_controller_type
               {$pcfg get_controller_type}}}
         {set
            [char_provider]
            {get_solo_char_provider
               {game get leader_num}}}
         {[char_provider]
            set_instrument_filter
            {$pcfg get_controller_sym}}
         {char.lst
            set_provider
            [char_provider]}
         {do
            ($char_pos
               {[char_provider]
                  get_pos_from_char
                  {$pcfg get_char}})
            {unless
               {== $char_pos -1}
               {char.lst set_selected $char_pos}}}
         {band_ui_sink
            enable_lefty_override
            {$pcfg get player_num}
            TRUE}}
      {$this set_gender FALSE}
      {$this update})
   (exit
      {do
         ($pcfg
            {game
               get_player_config
               {game get leader_num}})
         {band_ui_sink
            enable_lefty_override
            {$pcfg get player_num}
            FALSE}})
   (unload
      {charcreator_panel free_all})
   (update
      {do
         ($pos
            {char.lst selected_pos})
         ($pcfg
            {game
               get_player_config
               {game get leader_num}})
         ($char
            {if_else
               {[char_provider]
                  is_create_new
                  $pos}
               0
               {[char_provider]
                  get_char
                  $pos}})
         {$pcfg set_char $char}
         {if
            $char
            {char_cache
               request
               {game get leader_num}
               {$pcfg get_char}}}})
   (SCROLL_MSG
      {$this update})
   (FOCUS_MSG
      {if_else
         {'||'
            {== male.btn $new_focus}
            {== female.btn $new_focus}}
         {charcreator_panel
            request
            {game get leader_num}
            {basename
               {$new_focus name}}}
         {charcreator_panel
            free
            {game get leader_num}}})
   (SELECT_MSG
      {if_else
         {== $component char.lst}
         {if_else
            {[char_provider]
               is_create_new
               {$component selected_pos}}
            {if_else
               {profile_mgr
                  can_player_create_char
                  {game get leader_num}
                  {{game
                        get_player_config
                        {game get leader_num}}
                     get_controller_sym}}
               {$this set_gender TRUE}
               {ui push_screen tour_solo_char_no_room_screen}}
            {do
               {tour_solo_hub_panel set reset TRUE}
               {ui
                  goto_screen
                  {gamemode get post_matchmaking_screen}}}}
         {do
            {charcreator_panel
               set
               char_player_num
               {game get leader_num}}
            {ui goto_screen charcreator_maker_screen}}})
   (set_gender
      ($enable)
      {char.grp
         set_showing
         {! $enable}}
      {gender.grp set_showing $enable}
      {$this
         set_focus
         {if_else $enable male.btn char.lst}})}
