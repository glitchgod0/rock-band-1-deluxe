{new
   UIPanel
   tour_solo_gigcomplete_panel
   (file
      "tour_solo_gigcomplete.milo")
   (focus continue.btn)
   LIMIT_TO_LEADER_SESSION
   (enter
      {icons.grp set_frame 0}
      {platform_mgr set_notify_ui_location kOSNotifyBottomCenter}
      {do
         ($tp
            {tour performer})
         ($performer
            {beatmatch active_player 0})
         ($event
            {tour event})
         {stars.lbl
            set_localized
            {$performer star_rating}}
         {score.lbl
            set_localized
            {fmt_score
               {$performer score}}}
         {money.lbl
            set_localized
            {sprintf
               {localize tour_solo_reward}
               {fmt_money
                  {$tp reward}}}}
         {do
            ($instrument
               {$tp get_instrument})
            ($gender
               {$tp get_gender})
            ($category
               {$event unlock_category $instrument $gender})
            ($outfit
               {$event unlock_outfit $instrument $gender})
            ($line_idx 0)
            {if
               {&&
                  {!= $category ''}
                  {!
                     {$tp has_outfit $category $outfit}}}
               {{$this icon $line_idx}
                  set_showing
                  TRUE}
               {{$this icon $line_idx}
                  set_mat
                  icon_outfit.mat}
               {{$this line $line_idx}
                  set_showing
                  TRUE}
               {{$this line $line_idx}
                  set_localized
                  {sprintf
                     {localize
                        {if_else
                           {'||'
                              {== $category guitar}
                              {== $category drum}
                              {== $category bass}
                              {== $category mic}}
                           tour_solo_instrument
                           tour_solo_outfit}}
                     {localize_token
                        {shop find_token $gender $category $outfit}}}}
               {'++' $line_idx}}
            {{tour performer}
               complete_event}
            {cond
               ({!=
                     {$tp new_town}
                     ''}
                  {{$this icon $line_idx}
                     set_showing
                     TRUE}
                  {{$this icon $line_idx}
                     set_mat
                     icon_newtown.mat}
                  {{$this line $line_idx}
                     set_showing
                     TRUE}
                  {{$this line $line_idx}
                     set_localized
                     {sprintf
                        {localize tour_endgame_newtown}
                        {localize
                           {$tp new_town}}}}
                  {'++' $line_idx})
               ({!=
                     {$tp new_unlock}
                     ''}
                  {{$this icon $line_idx}
                     set_showing
                     TRUE}
                  {{$this icon $line_idx}
                     set_mat
                     icon_newtown.mat}
                  {{$this line $line_idx}
                     set_showing
                     TRUE}
                  {{$this line $line_idx}
                     set_localized
                     {sprintf
                        {localize tour_solo_newsong}
                        {song_mgr
                           song_name
                           {$tp new_unlock}}}}
                  {'++' $line_idx})}
            {foreach_int
               $i
               $line_idx
               2
               {{$this icon $i}
                  set_showing
                  FALSE}
               {{$this line $i}
                  set_showing
                  FALSE}}}
         {log_tour
            "EVENT   "
            {sprintf
               "WON $%d"
               {{tour performer}
                  reward}}}
         {log_tour
            "EVENT   "
            {sprintf
               "WON *x%d"
               {$performer num_stars}}}
         {foreach
            $venue
            (big_club theater arena)
            {if
               {$tp is_venue_type_unlocked $venue}
               {achievements
                  submit
                  {game get leader_num}
                  {symbol
                     {sprintf
                        "ach_solo_venue_%s"
                        $venue}}}}}})
   (exit
      {platform_mgr set_notify_ui_location kOSNotifyTopRight})
   (line
      ($idx)
      {sprintf
         "line%i.lbl"
         $idx})
   (icon
      ($idx)
      {sprintf
         "icon%i.mesh"
         $idx})
   (SELECT_START_MSG
      {ui
         goto_screen
         {if_else
            {tour get win_sequence}
            meta_loading_tour_win_screen
            meta_loading_continue_screen}}
      kDataUnhandled)}
