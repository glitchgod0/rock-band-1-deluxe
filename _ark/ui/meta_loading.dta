{new
   UIPanel
   meta_loading
   (file meta_loading.milo)}
#define AUTOSAVE_WAIT_SCREEN_HANDLERS
((next_screen '')
   (show_error FALSE)
   (entered FALSE)
   (enter
      {set
         [entered]
         TRUE}
      {game
         foreach_local_player_config
         $pcfg
         {$pcfg set_ui_waiting kUIWaitPostAutosave}}
      {session add_sink $this}
      {$this check_advance})
   (exit
      {game
         foreach_local_player_config
         $pcfg
         {$pcfg set_ui_waiting kUIWaitNone}}
      {session remove_sink $this})
   (REMOTE_PLAYER_CHANGED_MSG
      {$this check_advance})
   (REMOTE_PLAYER_LEFT_MSG
      {unless
         {session disabled}
         {$this check_advance}})
   (check_advance
      {if
         [entered]
         {do
            ($ready TRUE)
            {game
               foreach_player_config
               $pcfg
               {if
                  {&&
                     {session
                        has_player
                        {$pcfg get player_num}}
                     {!=
                        {$pcfg ui_waiting}
                        kUIWaitPostAutosave}
                     {!
                        {gamemode get ranked}}}
                  {set $ready FALSE}}}
            {if
               $ready
               {band_ui_sink set ui_disabled FALSE}
               {set
                  [entered]
                  FALSE}
               {ui
                  sync_screen
                  [next_screen]
                  0}}}}))
{new
   UIScreen
   autosave_wait_screen
   (panels meta_loading)
   AUTOSAVE_WAIT_SCREEN_HANDLERS}
{new
   UIScreen
   autosave_wait_in_meta_screen
   (panels meta_loading meta)
   AUTOSAVE_WAIT_SCREEN_HANDLERS}
#define META_LOADING_TRANSITION_HANDLERS
((transition_screen
      {object
         ""})
   (show_error FALSE)
   (entered FALSE)
   (ready FALSE)
   (enter
      {set
         [entered]
         TRUE}
      {set
         [ready]
         FALSE})
   (check_waiting
      {do
         ($waiting TRUE)
         {game
            foreach_player_config
            $pcfg
            {if
               {&&
                  {!=
                     {band_ui_sink current_event}
                     sign_out}
                  {session
                     has_player
                     {$pcfg get player_num}}
                  {==
                     {$pcfg ui_waiting}
                     kUIWaitNone}
                  {!
                     {gamemode get ranked}}}
               {set $waiting FALSE}}}
         $waiting})
   (poll
      {if
         {&&
            {char_cache meta_configure_done}
            {!
               {ui in_transition}}
            {'||'
               [entered]
               [ready]}}
         {if
            [entered]
            #ifndef _SHIP
            {char_cache check_outside_refs}
            #endif
            {if
               {[transition_screen]
                  get
                  clear_game_state}
               {session
                  clear
                  {[transition_screen]
                     get
                     session_leaving_early}}
               {tour clear_performer}
               {gamecfg reset_config}
               {if
                  {band_ui_sink get invite_accepted}
                  {gamemode
                     set_mode
                     {{session_searcher get_last_invite_result}
                        get_mode_name}}}}
            {gamecfg set_active_roster FALSE}
            {game
               foreach_local_player_config
               $pcfg
               {$pcfg set_ui_waiting kUIWaitPreAutosave}}
            {set
               [ready]
               TRUE}}
         {set
            [entered]
            FALSE}
         {if
            {&&
               [ready]
               {$this check_waiting}}
            {band_ui_sink set_block_mode kBlockNone}
            {do
               ($autosave_screen
                  {if_else
                     {meta is_up}
                     autosave_wait_in_meta_screen
                     autosave_wait_screen})
               ($was_disabled
                  {band_ui_sink get ui_disabled})
               {band_ui_sink set ui_disabled FALSE}
               {$autosave_screen
                  set
                  next_screen
                  {[transition_screen]
                     get
                     next_screen}}
               {autosave_push_goto $autosave_screen}
               {if
                  $was_disabled
                  {band_ui_sink set ui_disabled TRUE}}
               {set
                  [ready]
                  FALSE}}}}))
{new
   UIScreen
   meta_loading_screen
   (panels meta_loading)
   META_LOADING_TRANSITION_HANDLERS}
{new
   UIScreen
   meta_loading_in_meta_screen
   (panels meta_loading meta)
   META_LOADING_TRANSITION_HANDLERS}
#define META_LOADING_SCREEN_HANDLERS
((panels meta_loading)
   (loading_screen '')
   (delay_frames -1)
   (show_error FALSE)
   (load_panels
      {if
         [clear_game_state]
         {session ignore_remote_messages}})
   (enter
      {set
         [delay_frames]
         3}
      {set
         [loading_screen]
         {if_else
            {meta is_up}
            meta_loading_in_meta_screen
            meta_loading_screen}}
      {[loading_screen]
         set
         transition_screen
         $this}
      {unless
         {== $this meta_loading_matchmaking_screen}
         {band_ui_sink set invite_accepted FALSE}}
      {band_ui_sink set_block_mode kBlockLoadingGame})
   (exit
      {rnd set_in_game FALSE})
   (poll
      {--
         [delay_frames]}
      {if
         {==
            [delay_frames]
            0}
         {set
            [delay_frames]
            -1}
         {band_ui_sink set_net_sync FALSE}
         {do
            ($was_disabled
               {band_ui_sink get ui_disabled})
            {band_ui_sink set ui_disabled FALSE}
            {band_ui_sink
               force_reset_screen
               [loading_screen]}
            {if
               $was_disabled
               {band_ui_sink set ui_disabled TRUE}}}}))
{new
   UIScreen
   meta_loading_continue_screen
   META_LOADING_SCREEN_HANDLERS
   (clear_game_state FALSE)
   (session_leaving_early FALSE)
   (next_screen
      {gamemode get continue_screen})}
