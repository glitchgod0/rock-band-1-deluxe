#define kMetaSaveUnloaded
(0)
#define kMetaSaveLoaded
(1)
#define kMetaSaveError
(2)
#define kMetaSaveDelete
(3)
#define kMetaSaveUnchanged
(-1)
#define kAutoSave
(1)
#define kAutoLoad
(2)
#define kManualSave
(3)
#define kManualLoad
(4)
#define kMetaNextTypePushPop
(0)
#define kMetaNextTypePushPopGoto
(1)
#define kMetaNextTypeGotoPop
(2)
#define kMetaNextTypeGotoGoto
(3)
#define kMetaNextTypeGotoPopGoto
(4)
{func
   autoload
   {profile_mgr purge_old_data}
   #ifdef HX_PS3
   {ps3_autoload_start TRUE}
   #else
   {autoload_look_for_new_signin TRUE}
   #endif}
{func
   autoload_deferred_save
   {profile_mgr purge_old_data}
   #ifdef HX_PS3
   {ps3_autoload_start FALSE}
   #else
   {autoload_look_for_new_signin FALSE}
   #endif}
{func
   autosave
   #ifdef HX_PS3
   {ps3_autosave_start kMetaNextTypePushPop ''}
   #else
   {xbox_autosave_start kMetaNextTypePushPop ''}
   #endif}
