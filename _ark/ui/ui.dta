#include cam.dta
(SELECT_START_MSG
   {synth play button_select}
   kDataUnhandled)
(SCROLL_MSG
   {if
      {ui current_screen}
      {do
         ($play
            {handle_ret
               ({ui current_screen}
                  scroll_sfx)})
         {if
            {'||'
               {== $play kDataUnhandled}
               $play}
            {synth play button_toggle}}}}
   kDataUnhandled)
(SCROLL_SELECT_MSG
   {synth
      play
      {if_else $selected button_select button_back}}
   kDataUnhandled)
(FOCUS_MSG
   {unless
      {ui in_transition}
      {synth play button_toggle}}
   kDataUnhandled)
(BAD_SELECT_MSG
   {synth play button_error}
   kDataUnhandled)
(SCREEN_CHANGE_MSG
   {if
      {&&
         $back
         {meta is_up}}
      {synth play button_back}}
   kDataUnhandled)
(TRANSITION_COMPLETE_MSG
   {if
      {helpbar is_up}
      {helpbar update $new_screen}}
   {if
      {&&
         {fx is_up}
         {fx get in_transition}}
      {fx fade_in}}
   kDataUnhandled)
(update_band
   {update_tour_display}
   kDataUnhandled)
(JOYPAD_CONNECT_MSG
   {if
      {helpbar is_up}
      {helpbar joypad_connect $player_num $connected}}
   kDataUnhandled)
(BUTTON_DOWN_MSG
   {handle
      (profile_mgr button_down $button $player_num)}
   kDataUnhandled)
